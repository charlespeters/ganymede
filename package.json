{
  "private": true,
  "name": "ganymede-atom-syntax",
  "theme": "syntax",
  "version": "2.0.2",
  "devDependencies": {
    "prettier": "^1.14.3"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "workspaces": [
    "ganymede",
    "ganymede-vscode-theme",
    "next",
    "hyperganymede",
    "iterm",
    "vim"
  ],
