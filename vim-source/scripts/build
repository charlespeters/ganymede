#!/bin/bash
set -euo pipefail

warning='" WARNING: Do not edit this file directly - it is built from the src directory'
echo $warning > colors/ganymede.vim
echo $warning > autoload/airline/themes/ganymede.vim
echo $warning > autoload/lightline/colorscheme/ganymede.vim
node src/index.js >> colors/ganymede.vim
node src/airline.js >> autoload/airline/themes/ganymede.vim
node src/lightline.js >> autoload/lightline/colorscheme/ganymede.vim
# mv colors ../../
# mv autoload ../../
