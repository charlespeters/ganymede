// Place your settings in this file to overwrite default and user settings.
{
  "tokenColors": [
    {
      "name": "Global settings",
      "settings": {
        "background": "#12171F",
        "foreground": "#EFEFEF"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#F25A55"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#00B7ED"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#F1D677"
      }
    },
    {
      "name": "Identifier",
      "scope": "variable, support.variable, support.class, support.constant, meta.definition.variable entity.name.function",
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword, modifier, variable.language.this, support.type.object, constant.language",
      "settings": {
        "foreground": "#00B7ED"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function, support.function",
      "settings": {
        "foreground": "#F1D677"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.type, storage.modifier",
      "settings": {
        "foreground": "#00B7ED"
      }
    },
    {
      "name": "Modules",
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": "support.type",
      "settings": {
        "foreground": "#FFBA00"
      }
    },
    {
      "name": "Type",
      "scope": "entity.name.type, entity.other.inherited-class",
      "settings": {
        "foreground": "#FFBA00"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#36455d",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#FFBA00",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Class variable",
      "scope": "variable.object.property",
      "settings": {
        "foreground": "#FFBA00"
      }
    },
    {
      "name": "Class method",
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#FFBA00"
      }
    },
    {
      "name": "Function definition",
      "scope": "meta.function entity.name.function",
      "settings": {
        "foreground": "#FFBA00"
      }
    },
    {
      "name": "Template expression",
      "scope": "template.expression.begin, template.expression.end",
      "settings": {
        "foreground": "#00B7ED"
      }
    },
    {
      "name": "YAML key",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "JSON key",
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "JSON constant",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#00B7ED"
      }
    },
    {
      "name": "CSS class",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#00B7ED"
      }
    },
    {
      "name": "CSS ID",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#F25A55"
      }
    },
    {
      "name": "CSS tag",
      "scope": "source.css entity.name.tag",
      "settings": {
        "foreground": "#FFBA00"
      }
    },
    {
      "name": "HTML tag outer",
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#00B7ED"
      }
    },
    {
      "name": "HTML tag inner",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "HTML tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#F1D677"
      }
    },
    {
      "name": "Markdown heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#00B7ED"
      }
    },
    {
      "name": "Markdown link text",
      "scope": "text.html.markdown meta.link.inline, meta.link.reference",
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "Markdown list item",
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#00B7ED"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold italic",
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "INI property name",
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "INI section title",
      "scope": "entity.name.section.group-title.ini",
      "settings": {
        "foreground": "#00B7ED"
      }
    },
    {
      "name": "C# class",
      "scope": "source.cs meta.class.identifier storage.type",
      "settings": {
        "foreground": "#FFBA00",
        "fontStyle": "underline"
      }
    },
    {
      "name": "C# class method",
      "scope": "source.cs meta.method.identifier entity.name.function",
      "settings": {
        "foreground": "#FFBA00"
      }
    },
    {
      "name": "C# function call",
      "scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
      "settings": {
        "foreground": "#F1D677"
      }
    },
    {
      "name": "C# type",
      "scope": "source.cs storage.type",
      "settings": {
        "foreground": "#FFBA00"
      }
    },
    {
      "name": "C# return type",
      "scope": "source.cs meta.method.return-type",
      "settings": {
        "foreground": "#FFBA00"
      }
    },
    {
      "name": "C# preprocessor",
      "scope": "source.cs meta.preprocessor",
      "settings": {
        "foreground": "#36455d"
      }
    },
    {
      "name": "C# namespace",
      "scope": "source.cs entity.name.type.namespace",
      "settings": {
        "foreground": "#EFEFEF"
      }
    },
    {
      "name": "Global settings",
      "settings": {
        "background": "#12171F",
        "foreground": "#EFEFEF"
      }
    }
  ],
